@{
    ViewData["Title"] = "Trang chủ";
     Layout = "_Layout_VTV";
}
@using LeMaiDomain.Models;
@model WebHomeIndexModel;

<main class="main">
    <!-- promo start-->
    <div class="promo">
        <div class="promo-slider">
            <div class="promo-slider__item">
                <div class="promo-slider__layout"></div>
                <picture>
                    <source srcset="~/vtv/img/promo_1.jpg" media="(min-width: 992px)" /><img class="img--bg" src="~/vtv/img/promo_1.jpg" alt="img" />
                </picture>
                <div class="align-container">
                    <div class="align-container__item">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-9 col-xl-7">
                                    <div class="promo-slider__wrapper-1">
                                        <span class="promo-slider__overlay">VTV</span>
                                        <h2 class="promo-slider__title">Vận chuyển hàng hóa quốc tế</h2>
                                    </div>
                                    <div class="promo-slider__wrapper-2">
                                        <p class="promo-slider__subtitle">Vận Thông Việt nhận vận chuyển hàng hóa đi tất cả các quốc gia trên toàn thế giới với thời gian nhanh chóng, giá vận chuyện cực kỳ thấp sẽ đáp ứng sự hài lòng của quý khách hàng.</p>
                                    </div>
                                    <div class="promo-slider__wrapper-3">
                                        <a class="button button--promo" href="#">
                                            <span>Chi tiết</span>
                                            <svg class="icon">
                                                <use xlink:href="#arrow"></use>
                                            </svg>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="promo-slider__item text-center">
                <div class="promo-slider__layout"></div>
                <picture>
                    <source srcset="~/vtv/img/promo_2.jpg" media="(min-width: 992px)" /><img class="img--bg" src="~/vtv/img/promo_2.jpg" alt="img" />
                </picture>
                <div class="align-container">
                    <div class="align-container__item">
                        <div class="container">
                            <div class="row">
                                <div class="col-12">
                                    <div class="promo-slider__wrapper-1">
                                        <span class="promo-slider__overlay position-center">VTV</span>
                                        <h2 class="promo-slider__title">Chuyển phát nhanh toàn quốc</h2>
                                    </div>
                                    <div class="promo-slider__wrapper-2">
                                        <p class="promo-slider__subtitle">VTV Express là đơn vị chuyển phát nhanh đi 63 tỉnh thành trong cả nước với thời gian siêu tốc và giá cước phải chăng. Chúng tôi có những đối tác vận chuyển chuyên nghiệp và mạng lưới phủ khắp.</p>
                                    </div>
                                    <div class="promo-slider__wrapper-3">
                                        <a class="button button--promo" href="#">
                                            <span>Chi tiết</span>
                                            <svg class="icon">
                                                <use xlink:href="#arrow"></use>
                                            </svg>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="promo-slider__item">
                <div class="promo-slider__layout"></div>
                <picture>
                    <source srcset="~/vtv/img/promo_3.jpg" media="(min-width: 992px)" /><img class="img--bg" src="~/vtv/img/promo_3.jpg" alt="img" />
                </picture>
                <div class="align-container">
                    <div class="align-container__item">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-9 col-xl-7">
                                    <div class="promo-slider__wrapper-1">
                                        <span class="promo-slider__overlay">VTV</span>
                                        <h2 class="promo-slider__title"> <br />in any Way</h2>
                                    </div>
                                    <div class="promo-slider__wrapper-2">
                                        <p class="promo-slider__subtitle">Rockling Devario deep sea bonefish cutthroat trout streamer fish kaluga sailback scorpionfish sand dab, turkeyfish golden loach sand diver. Leopard danio píntano bonnetmouth; blue whiting, suckermouth armored catfish luderick blackchin kingfish.</p>
                                    </div>
                                    <div class="promo-slider__wrapper-3">
                                        <a class="button button--promo" href="#">
                                            <span>Chi tiết</span>
                                            <svg class="icon">
                                                <use xlink:href="#arrow"></use>
                                            </svg>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="promo__lower">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-md-5 col-lg-7 col-xl-8 d-flex align-items-start">
                        <div class="promo-slider__nav"></div>
                        <div class="promo-slider__count"></div>
                    </div>
                    <div class="col-md-7 col-lg-5 col-xl-4 top-20 top-md-0">
                        <form class="tracking-form" asp-action="Index" asp-controller="VTV" autocomplete="off" method="post">
                            <label class="tracking-form__label">
                                <input class="tracking-form__input" type="text" value="@Model.billCode" name="code" required="required" /><span class="tracking-form__placeholder">@ViewData["tracuulable"]</span>
                            </label>
                            <button class="tracking-form__submit" type="submit">
                                <svg class="icon">
                                    <use xlink:href="#arrow"></use>
                                </svg>
                            </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- promo end-->
    <!-- section start-->
    <section class="section">
        <div class="container">
            <div class="row" style="background-color:#e8f6fb">
                <div class="containerTracking" style="color:black; @Model.display">
                    <div style="@Model.displayInfo">
                        <div class="titleTracking" style="font-size: 18px;color:#00044B;padding-top: 10px;padding-bottom: 5px;font-weight: bold;font-style: italic;">Thông tin đơn hàng</div>
                        <div style="padding:0px 0px 10px 5px"><span style="font-weight:bold">Người nhận:</span> @Model.AcceptMan - <span style="font-weight:bold">SĐT:</span> @Model.AcceptManPhone</div>
                        <div style="padding:0px 0px 10px 5px"><span style="font-weight:bold">Tên hàng:</span> @Model.GoodsName - <span style="font-weight:bold">Cân nặng:</span> @Model.BillWeight.ToString("N0") gram - <span style="font-weight:bold">Tổng tiền phải thu:</span> @Model.Total.ToString("N0") (COD: @Model.COD.ToString("N0") + SHIP: @Model.Freight.ToString("N0"))</div>
                    </div>
                    <div class="titleTracking" style="font-size: 18px;color:#00044B;padding-top: 10px;padding-bottom: 5px;font-weight: bold;font-style: italic;">@Model.message</div>
                    <div id="listTracking" style="padding:10px !important">
                        @foreach (var item in Model.listTracking)
                        {
                            if (item == Model.listTracking[0])
                            {
                                <div class="_item" style="color:red"><i class="fa-solid fa-road-circle-check"></i> @string.Format("{0:dd/MM/yyyy HH:mm}", item.CreateDate)</div>
                                <div class="_more" style="padding-left:20px;">
                                    <div><i class="fa-solid fa-signs-post"></i> @item.Note</div>
                                </div>
                            }
                            else
                            {
                                <div class="_item" style="color:dodgerblue"><i class="fa-solid fa-road-circle-check"></i> @string.Format("{0:dd/MM/yyyy HH:mm}", item.CreateDate)</div>
                                <div class="_more" style="padding-left:20px;">
                                    <div><i class="fa-solid fa-signs-post"></i> @item.Note</div>
                                </div>
                            }
                        }
                    </div>
                </div>
            </div>
            <div class="row bottom-70">
                <div class="col-lg-4">
                    <div class="heading bottom-40">
                        <span class="heading__pre-title">Dịch vụ</span>
                        <h3 class="heading__title">Chuyển phát nhanh</h3><span class="heading__layout">Services</span>
                    </div><a class="button button--green d-none d-lg-inline-block" href="#">
                        <span>Chi tiết</span>
                        <svg class="icon">
                            <use xlink:href="#arrow"></use>
                        </svg>
                    </a>
                </div>
                <div class="col-lg-8">
                    <p><strong>VTV Express cung cấp nhiều loại dịch vụ vận chuyển đa dạng như: Chuyển phát nhanh trong nước, quốc tế, chuyên chở hàng hóa bằng xe tải, tàu biển và đường hàng không.</strong></p>
                    <p class="bottom-0">
                        Với nhiều năm kinh nghiệm trong lĩnh vực vận chuyển, chúng tôi cung cấp đến quý khách hàng những dịch vụ chuyên nghiệp nhất, nhanh nhất với giá cước rẻ nhất. Ngoài dịch vụ vận chuyển
                        VTV Express còn cung cấp dịch vụ mua hộ hàng hóa nội địa trung quốc từ các nhà cung cấp sản phẩm uy tín, chất lượng cao phù hợp cho các shop cần nguồn hàng tốt giá rẻ.
                    </p>
                </div>
            </div>
            <div class="row offset-50">
                <div class="col-sm-6 col-xl-3">
                    <div class="count-item">
                        <div class="count-item__img"><img src="~/vtv/img/front-serv_1.png" alt="icon" /></div>
                        <h6 class="count-item__title"><span class="count-item__count">01</span> <span>Chuyển phát nhanh trong nước</span></h6>
                    </div>
                </div>
                <div class="col-sm-6 col-xl-3">
                    <div class="count-item">
                        <div class="count-item__img"><img src="~/vtv/img/front-serv_3.png" alt="icon" /></div>
                        <h6 class="count-item__title"><span class="count-item__count">03</span> <span>Chuyển phát nhanh quốc tế</span></h6>
                    </div>
                </div>
                <div class="col-sm-6 col-xl-3">
                    <div class="count-item">
                        <div class="count-item__img"><img src="~/vtv/img/front-serv_2.png" alt="icon" /></div>
                        <h6 class="count-item__title"><span class="count-item__count">02</span> <span>Vận chuyển đường biển</span></h6>
                    </div>
                </div>

                <div class="col-sm-6 col-xl-3">
                    <div class="count-item">
                        <div class="count-item__img"><img src="~/vtv/img/front-serv_4.png" alt="icon" /></div>
                        <h6 class="count-item__title"><span class="count-item__count">04</span> <span>Mua hàng trung quốc</span></h6>
                    </div>
                </div>
            </div>
            <div class="row top-70 d-flex d-lg-none">
                <div class="col-12 text-center">
                    <a class="button button--green" href="#">
                        <span>Tất cả dịch vụ</span>
                        <svg class="icon">
                            <use xlink:href="#arrow"></use>
                        </svg>
                    </a>
                </div>
            </div>
        </div>
    </section>
    <!-- section end-->
    <!-- section start-->
    <section class="section front-about">
        <div class="front-about__bg"><img class="section--bg t50 r0" src="~/vtv/img/testimonials-bg.png" alt="img" /></div>
        <div class="front-about__img">
            <img class="img--bg" src="~/vtv/img/about_img.jpg" alt="img" /><a class="js-video front-about__video" href="https://www.youtube.com/watch?v=Of7qg0MiR2I">
                <svg class="icon">
                    <use xlink:href="#play"></use>
                </svg> <span>Video giới thiệu</span>
            </a>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-xl-4 offset-xl-1 d-none d-xl-flex offset-50">
                    <div class="counter counter--filled">
                        <div class="counter__top"><span class="js-counter counter__count">8</span><span class="counter__subject">năm</span></div>
                        <div class="counter__lower"><span class="counter__details">Kinh nghiệm trong ngành vận chuyển</span></div>
                    </div>
                </div>
                <div class="col-xl-7">
                    <div class="heading heading--white">
                        <span class="heading__pre-title">Giới thiệu VTV</span>
                        <h3 class="heading__title">VTV Express là đơn vị <span class="color--green">vận chuyển</span> đa nền tảng dịch vụ</h3>
                    </div>
                    <p>
                        VTV Express là đơn vị chuyển phát nhanh trên đa nền tảng dịch vụ với hệ thống mạng lưới bưu cục
                        phủ khắp 63 tỉnh thành trên cả nước. Cùng với nhiều loại dịch vụ vận chuyển khác đáp ứng mọi nhu cầu của khách hàng
                    </p>
                    <p>
                        Với nhiều năm kinh nghiệm trong lĩnh vực vận chuyển, khác với các đơn vị vận chuyển hiện có tại thị trường chỉ chăm lo đơn hàng từ
                        những sàn thương mại điện tử lớn. Với VTV Express khách hàng là những shop tự bán hàng, đến với chúng tôi kháchh hàng sẽ được
                        nhân viên chăm sóc đơn hàng, xem đơn hàng của shop như là đơn hàng của chính mình. Chăm chút từ việc đóng gói cho đến vận chuyển, giao hàng...
                    </p>
                    <a class="button button--white" href="#">
                        <span>Xem thêm</span>
                        <svg class="icon">
                            <use xlink:href="#arrow"></use>
                        </svg>
                    </a>
                </div>
            </div>
        </div>
    </section>
    <!-- section end-->
    <!-- section start-->
    <section class="section bg--lgray">
        <div class="container">
            <div class="row align-items-end bottom-70">
                <div class="col-lg-5 col-xl-4">
                    <div class="heading">
                        <span class="heading__pre-title">Thế mạnh</span>
                        <h3 class="heading__title"> Thế mạnh của <span class="color--green">VTV</span> Express</h3><span class="heading__layout layout--lgray">Features</span>
                    </div>
                </div>
            </div>
            <div class="row offset-50">
                <div class="col-sm-6 col-lg-3">
                    <div class="icon-item">
                        <div class="icon-item__img">
                            <svg class="icon">
                                <use xlink:href="#warehouse"></use>
                            </svg>
                        </div>
                        <h6 class="icon-item__title">Mạng lưới bưu cục</h6>
                        <p class="icon-item__text">Là đơn vị vận chuyển đa nền tảng nên chúng tôi có một mạng lưới bưu cục phủ khắp cả nước.</p>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-3">
                    <div class="icon-item">
                        <div class="icon-item__img">
                            <svg class="icon">
                                <use xlink:href="#support"></use>
                            </svg>
                        </div>
                        <h6 class="icon-item__title">Hoạt động xuyên suốt 24/7</h6>
                        <p class="icon-item__text"> Hệ thống hoạt động xuyên suốt 24/7, đảm bảo đơn hàng được giao trong thời gian nhanh nhất.</p>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-3">
                    <div class="icon-item">
                        <div class="icon-item__img">
                            <svg class="icon">
                                <use xlink:href="#location"></use>
                            </svg>
                        </div>
                        <h6 class="icon-item__title">Kiểm tra trực tuyến</h6>
                        <p class="icon-item__text">Khách hàng có thể tra cứu tình trạng đơn hàng một cách nhanh trong bằng nhiều cách như zalo, website, app trên mobile.</p>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-3">
                    <div class="icon-item">
                        <div class="icon-item__img">
                            <svg class="icon">
                                <use xlink:href="#insurance"></use>
                            </svg>
                        </div>
                        <h6 class="icon-item__title">Hàng hóa an toàn</h6>
                        <p class="icon-item__text">Đến với dịch vụ của VTV hàng hóa của quý khách hàng luôn được nâng niu trong suốt quá trình vận chuyển</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- section end-->
    <!-- section start-->
    <section class="section">
        <div class="container">
            <div class="row">
                <div class="col-12"><img src="~/vtv/img/map-transx.png" alt="img" /></div>
            </div>
            <div class="row offset-50">
                <div class="col-md-6 col-lg-4 text-center">
                    <div class="counter counter--blue">
                        <div class="counter__top"><span class="js-counter counter__count">15</span><span class="counter__subject">triệu</span></div>
                        <div class="counter__lower"><span class="counter__details">Đơn giao thành công</span></div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-4 text-center">
                    <div class="counter counter--blue">
                        <div class="counter__top"><span class="js-counter counter__count">2</span><span class="counter__subject">nghìn</span></div>
                        <div class="counter__lower"><span class="counter__details">Số lượt vận tải mỗi năm</span></div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-4 text-center">
                    <div class="counter counter--blue">
                        <div class="counter__top"><span class="js-counter counter__count">600</span><span class="counter__subject">điểm</span></div>
                        <div class="counter__lower"><span class="counter__details">Tổng số bưu cục</span></div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- section end-->
    <!-- section start-->
    <section class="section qoute-section bg--lgray pb-0">
        <div class="quote-section__bg"><img class="img--bg" src="~/vtv/img/quote-bg.jpg" alt="bg" /></div>
        <div class="container">
            <div class="row">
                <div class="col-lg-5 d-flex flex-column justify-content-between">
                    <div class="heading heading--white">
                        <span class="heading__pre-title">Yêu cầu vận chuyển</span>
                        <h3 class="heading__title">Cung cấp nhanh thông tin yêu cầu vận chuyển</h3><span class="heading__layout layout--white">Yêu cầu</span>
                        <p class="heading__text color--white">Điền thông tin vào form bên cạnh về yêu cầu vận chuyển của bạn, chúng tôi sẽ tư vấn cho bạn ngay lập tức.</p>
                    </div>
                    <div class="contact-trigger top-50 top-lg-0">
                        <img class="contact-trigger__img" src="~/vtv/img/contact_background.png" alt="img" />
                        <h4 class="contact-trigger__title">How we can help you!</h4><a class="button button--white" href="#">
                            <span>Contact us</span>
                            <svg class="icon">
                                <use xlink:href="#arrow"></use>
                            </svg>
                        </a>
                    </div>
                </div>
                <div class="col-lg-7 top-50 top-lg-0">
                    <div class="form-wrapper">
                        <form class="form quote-form" action="javascript:void(0);">
                            <h6 class="quote-form__title">Yêu cầu vận chuyển</h6>
                            <div class="row">
                                <div class="col-sm-6">
                                    <input class="form__field" type="text" name="name" placeholder="Tên của bạn" />
                                </div>
                                <div class="col-sm-6">
                                    <input class="form__field" type="email" name="email" placeholder="Email" />
                                </div>
                                <div class="col-sm-6">
                                    <input class="form__field" type="tel" name="phone-number" placeholder="Số điện thoại" />
                                </div>
                                <div class="col-sm-6">
                                    <input class="form__field" type="text" name="subject" placeholder="Subject" />
                                </div>
                                <div class="col-sm-6">
                                    <input class="form__field" type="text" name="delivery" placeholder="Điểm nhận hàng" />
                                </div>
                                <div class="col-sm-6">
                                    <input class="form__field" type="text" name="departure" placeholder="Điểm đến" />
                                </div>
                                <div class="col-sm-6">
                                    <select class="form__select" name="freight">
                                        <option data-display="Freight type">Loại hàng hóa</option>
                                        <option value="value">Thông thường</option>
                                        <option value="value2">Đông lạnh</option>
                                        <option value="value3">Điện tử (dễ vỡ)</option>
                                        <option value="value3">Nông sản</option>
                                        <option value="value3">Chất lỏng</option>
                                    </select>
                                </div>
                                <div class="col-sm-6">
                                    <select class="form__select" name="incoterms">
                                        <option data-display="Incoterms">Loại dịch vụ</option>
                                        <option value="value">Chuyển phát nhanh</option>
                                        <option value="value2">Vận chuyển bằng xe</option>
                                        <option value="value3">Vận chuyển bằng tàu</option>
                                        <option value="value3">Vận chuyển bằng máy bay</option>
                                        <option value="value3">Vận chuyển bằng đường sắt</option>
                                    </select>
                                </div>
                                <div class="col-6 col-sm-3">
                                    <input class="form__field" type="number" name="weight" placeholder="Cân nặng" />
                                </div>
                                <div class="col-6 col-sm-3">
                                    <input class="form__field" type="number" name="height" placeholder="Cao" />
                                </div>
                                <div class="col-6 col-sm-3">
                                    <input class="form__field" type="number" name="width" placeholder="Rộng" />
                                </div>
                                <div class="col-6 col-sm-3">
                                    <input class="form__field" type="number" name="lenght" placeholder="Dài" />
                                </div>
                                <div class="col-12 offset-20">
                                    <label class="form__checkbox-label bottom-20">
                                        <span class="form__label-text">Dễ vỡ</span>
                                        <input class="form__input-checkbox" type="checkbox" name="size-select" value="Size L" /><span class="form__checkbox-mask"></span>
                                    </label>
                                    <label class="form__checkbox-label bottom-20">
                                        <span class="form__label-text">Cần gấp</span>
                                        <input class="form__input-checkbox" type="checkbox" name="size-select" value="Size S" /><span class="form__checkbox-mask"></span>
                                    </label>
